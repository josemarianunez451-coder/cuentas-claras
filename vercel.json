{
  "version": 2,
  "functions": {
    "backend/index.js": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/index.js"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/frontend/dist/index.html"
    }
  ]
}